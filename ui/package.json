    "dev": "npx nx dev web-app",
    "start": "npx nx start",
    "sass": "npx nx sass",
    "build": "npx nx build",
    "test": "jest",
    "test:watch": "jest --watch --transformIgnorePatterns 'node_modules/(?!(react-markdown.*|vfile.*|unified.*|rehype.*|trim-lines.*|remark.*|unist.*|hast.*|bail.*|trough.*|mdast.*|micromark.*|decode.*|character.*|property.*|space.*|comma.*|hljs.*|remark.*|is-plain-obj.*|remark-parse|mdast.*)/)'",
    "postinstall": "remix setup node",
    "icons:convert-font": "svg2ttf ./apps/web-app/app/assets/icons/output/custom-icons.svg ./apps/web-app/app/assets/icons/output/custom-icons.ttf && ttf2woff ./apps/web-app/app/assets/icons/output/custom-icons.ttf ./apps/web-app/app/assets/icons/output/custom-icons.woff",
    "icons:build-css": "node ./scripts/svgfont-to-css.js",
    "icons:build-font": "svgicons2svgfont --fontname tauruseer-custom-icons --output ./apps/web-app/app/assets/icons/output/custom-icons.svg --prependUnicode --normalize ./apps/web-app/app/assets/icons/svg/*.svg",
    "icons:build": "npm run icons:build-font && npm run icons:convert-font && npm run icons:build-css"
  },
  "private": true,
